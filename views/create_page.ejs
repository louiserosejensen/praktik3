<% include ./partials/header %>
<section class="admin-pages">
    <h1>Create pages</h1>
    <form id="pages_form" method="post" class="form-inline">
        <div class="form-group">
            <label for="pages_name">Page Name</label>
            <input type="text" name="pages_name" class="form-control" id="pages_name" value="<%= 
                (typeof(mainResults) != 'undefined' && typeof(mainResults.pages_name) != 'undefined') ? mainResults.pages_name : '' %>">
        </div>
        <div class="form-group">
            <label for="pages_description">Page Description</label>
            <textarea name="description" name="pages_description" class="form-control" id="pages_description"><%=
                (typeof(mainResults) != 'undefined' && typeof(mainResults.pages_description) != 'undefined') ? mainResults.pages_description : ''
            %></textarea>
        </div>
        <div class="form-group">
            <button type="submit">Upload</button>
        </div>
    </form>
</section>
<% include ./partials/footer %>