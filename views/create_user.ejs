<% include ./partials/header %>
<section class="admin-users">
    <h1>Create users</h1>
    <form id="users_form" method="post" class="form-inline">
        <div class="form-group">
            <label for="user_email">Email</label>
            <input type="text" name="user_email" class="form-control" id="user_email">
        </div>
        <div class="form-group">
            <label for="user_role_level">User Role</label>
            <input type="text" name="user_role_level" class="form-control" id="user_role_level">
        </div>
        <div class="form-group">
            <button type="submit">Upload</button>
        </div>
    </form>
</section>
<% include ./partials/footer %>